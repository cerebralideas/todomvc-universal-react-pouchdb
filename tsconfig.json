{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"moduleResolution": "node",
		"isolatedModules": false,
		"jsx": "react",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": false
	},
	"exclude": [
		"node_modules",
		"definitions"
	]
}
