{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "react",
		"module": "commonjs",
		"moduleResolution": "node",
		"removeComments": true,
		"target": "es2015",
		"sourceMap": true
	},
	"exclude": [
		"node_modules",
		"definitions",
		"/**/*.test.ts",
		"/**/*.test.tsx"
	]
}
