{
  "name": "universal-todomvc",
  "version": "2.0.0",
  "description": "A universal TodoMVC with React, Redux built with TypeScript.",
  "main": "./app/index.server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebralix/universal-todomvc"
  },
  "author": {
    "name": "Justin Lowery",
    "email": "justin@cix.io"
  },
  "contributors": [],
  "publishConfig": {
    "registry": "http://npm.com"
  },
  "scripts": {
    "build": "npm run lint && npm run transpile && npm run bundle",
    "build:dev": "npm run lint && npm run transpile && npm run bundle:dev",
    "bundle": "webpack --mode production",
    "bundle:dev": "webpack --mode development",
    "clean": "rm -rf node_modules && git clean -fX",
    "clean:data": "rm -rf users",
    "lint": "eslint '**/*.ts' --fix",
    "start": "NODE_ENV=production node app/index.server.js",
    "start:dev": "node app/index.server.js",
    "test": "jest",
    "transpile": "tsc",
    "watch": "chokidar -i 'node_modules' '**/*.{ts,tsx}' -c 'npm run build:dev' --initial"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run transpile",
      "pre-push": "npm run test && npm run build"
    }
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "multer": "^1.1.0",
    "page": "^1.7.1",
    "pouchdb": "^7.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-engine": "^4.5.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "rxjs": "^6.5.2",
    "serve-favicon": "^2.3.0",
    "superagent": "^5.1.0",
    "todomvc-app-css": "^2.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-serve-static-core": "^4.16.7",
    "@types/jest": "^24.0.15",
    "@types/mime": "^2.0.1",
    "@types/multer": "^1.3.7",
    "@types/page": "^1.8.0",
    "@types/pouchdb": "^6.3.3",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.6.1",
    "@types/serve-favicon": "^2.2.30",
    "@types/serve-static": "^1.13.2",
    "@types/superagent": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/typescript-estree": "^1.11.0",
    "acorn": "^6.2.0",
    "chokidar-cli": "^2.0.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^1.18.2",
    "proxyquire": "^1.7.3",
    "react-axe": "^3.2.0",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
