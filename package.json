{
  "name": "universal-todomvc",
  "version": "2.0.0",
  "description": "A universal TodoMVC with React, Redux built with TypeScript.",
  "main": "./app/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebralix/universal-todomvc"
  },
  "author": {
    "name": "Justin Lowery",
    "email": "justin@cix.io"
  },
  "contributors": [],
  "publishConfig": {
    "registry": "http://npm.com"
  },
  "scripts": {
    "build": "npm run transpile && npm run bundle",
    "bundle": "webpack",
    "dev": "npm run lint && npm run transpile",
    "lint": "tslint {lib,tests}/*.ts index.ts -r node_modules/tslint-microsoft-contrib",
    "stylecheck": "jscs {lib,tests}/*.js",
    "start": "node server.js",
    "test": "tape tests/**/*.test.js",
    "transpile": "tsc",
    "watch": "chokidar -i 'node_modules' '**/*.{ts,tsx}' -c 'npm run build' --initial"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "pre-push": ""
    }
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "classnames": "^2.1.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "multer": "^1.1.0",
    "page": "^1.7.1",
    "pouchdb": "^7.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-engine": "^4.5.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "rxjs": "^6.5.2",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.8.3",
    "todomvc-app-css": "^2.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/classnames": "^2.2.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-serve-static-core": "^4.16.7",
    "@types/mime": "^2.0.1",
    "@types/multer": "^1.3.7",
    "@types/page": "^1.8.0",
    "@types/pouchdb": "^6.3.3",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.6.1",
    "@types/serve-favicon": "^2.2.30",
    "@types/serve-static": "^1.13.2",
    "@types/superagent": "^4.1.1",
    "@types/tape": "^4.2.33",
    "chokidar-cli": "^1.2.0",
    "ghooks": "^1.0.3",
    "jscs": "^2.7.0",
    "json-loader": "^0.5.7",
    "proxyquire": "^1.7.3",
    "tape": "^4.4.0",
    "typescript": "^3.5.2",
    "webpack": "^1.15.0",
    "webpack-cli": "^3.3.5"
  }
}
