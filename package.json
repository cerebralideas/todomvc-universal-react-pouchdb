{
  "name": "commonweb-setup",
  "version": "0.0.1",
  "description": "NPM module for setting up a CommonWeb project.",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.paypal.com/CommonWeb-R/commonweb-setup"
  },
  "contributors": [
    {
      "name": "Team Six",
      "email": "DL-PayPal-TeamSix-Core@paypal.com",
      "url": "https://rally1.rallydev.com/#/11744709963d/teamstatus"
    }
  ],
  "publishConfig": {
    "registry": "http://npm.paypal.com"
  },
  "scripts": {
    "transpile": "tsc",
    "bundle": "tsc --jsx react -m system --outFile ./bundle.js ./client.tsx --sourceMap",
    "lint": "tslint {lib,tests}/*.ts index.ts -r node_modules/tslint-microsoft-contrib",
    "stylecheck": "jscs {lib,tests}/*.js",
    "test": "tape tests/**/*.test.js",
    "dev": "npm run lint && npm run transpile",
    "build": "npm run transpile && npm run bundle",
    "watch": "chokidar -i 'node_modules' '**/*.{ts,tsx}' -c 'npm run build' --initial"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "pre-push": ""
    }
  },
  "dependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "body-parser": "^1.15.0",
    "classnames": "^2.1.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "lodash": "^4.7.0",
    "multer": "^1.1.0",
    "page": "^1.7.1",
    "pouchdb": "^5.3.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-engine": "^3.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.1",
    "redux": "^3.4.0",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.8.3",
    "todomvc-app-css": "^2.0.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "ghooks": "^1.0.3",
    "jscs": "^2.7.0",
    "proxyquire": "^1.7.3",
    "tape": "^4.4.0",
    "typescript": "^1.9.0-dev.20160403",
    "webpack": "^1.12.14"
  },
  "maintainers": [
    {
      "name": "jlowery",
      "email": "jlowery@paypal.com"
    }
  ]
}
